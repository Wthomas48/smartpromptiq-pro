{
    "name": "smartpromptiq-saas",
    "version": "1.0.0",
    "description": "AI-Powered Prompt Engineering SaaS Platform",
    "type": "commonjs",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "cd backend && node simple-server.js",
        "dev:client": "cd client && npm run dev",
        "build:client": "cd client && npm run build",
        "install:all": "npm install && cd client && npm install && cd ../backend && npm install",
        "db:generate": "drizzle-kit generate",
        "db:push": "drizzle-kit push",
        "db:migrate": "drizzle-kit migrate",
        "test": "vitest",
        "lint": "eslint server --ext .ts",
        "type-check": "tsc --noEmit",
        "db:helper": "tsx scripts/db-helper.ts",
        "start": "node dist/server/index.js",
        "build": "npm run build:frontend && npm run build:server",
        "db:shell": "docker-compose exec postgres psql -U postgres -d smartpromptiq",
        "db:stop": "docker-compose down",
        "dev:local": "dotenv -e .env.local -- tsx watch server/index.ts",
        "dev:frontend": "vite",
        "db:logs": "docker-compose logs -f postgres",
        "dev:staging": "dotenv -e .env.staging -- tsx watch server/index.ts",
        "build:frontend": "vite build",
        "db:start": "docker-compose up -d",
        "build:server": "tsc -p tsconfig.server.json",
        "db:reset": "docker-compose down -v && docker-compose up -d",
        "db:utils": "tsx scripts/db-utils.ts"
    },
