{
    "name": "smartpromptiq-saas",
    "version": "1.0.3",
    "description": "AI-Powered Prompt Engineering SaaS Platform",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "cd backend && npm run dev",
        "dev:client": "cd client && npm run dev",
        "prebuild": "node -e \"process.env.BUILD_TIME=new Date().toISOString();console.log('Build time set to:', process.env.BUILD_TIME)\"",
        "build": "npm run generate && npm run build:client && npm run build:server",
        "build:client": "cd client && npm install && npm run build",
        "build:server": "echo 'Server build complete'",
        "generate": "npx prisma generate --schema=./backend/prisma/schema.prisma",
        "postinstall": "npx prisma generate --schema=./backend/prisma/schema.prisma || echo 'Prisma generate skipped'",
        "install:deps": "cd client && npm install",
        "install:client": "cd client && npm install",
        "install:all": "npm install && cd client && npm install && cd ../backend && npm install",
        "start": "node index.cjs",
        "deploy": "node deploy-all.js",
        "deploy:frontend": "node deploy-frontend.js",
        "deploy:backend": "node deploy-railway.js",
        "deploy:railway": "node deploy-railway.js",
        "deploy:all": "npm run build:client && node deploy-all.js",
        "db:generate": "drizzle-kit generate",
        "db:push": "drizzle-kit push",
        "db:migrate": "drizzle-kit migrate",
        "db:seed": "cd backend && npm run seed",
        "db:helper": "tsx scripts/db-helper.ts",
        "db:shell": "docker-compose exec postgres psql -U postgres -d smartpromptiq",
        "db:stop": "docker-compose down",
        "db:logs": "docker-compose logs -f postgres",
        "db:start": "docker-compose up -d",
        "db:reset": "docker-compose down -v && docker-compose up -d",
        "db:utils": "tsx scripts/db-utils.ts",
        "admin:create": "node create-admin-sqlite.cjs",
        "admin:create:prod": "node setup-production-admin.cjs",
        "admin:verify": "node verify-admin.cjs",
        "admin:check": "node check-db.cjs",
        "railway:build": "cd client && rm -rf node_modules && npm install && npm run build",
        "railway:start": "npm start",
        "railway:admin": "node setup-production-admin.cjs",
        "dev:local": "dotenv -e .env.local -- tsx watch server/index.ts",
        "dev:frontend": "vite",
        "dev:staging": "dotenv -e .env.staging -- tsx watch server/index.ts",
        "build:frontend": "npm run build:client",
        "test": "vitest",
        "lint": "eslint server --ext .ts",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@prisma/client": "^6.17.1",
        "prisma": "^6.17.1",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "express": "^4.21.2",
        "express-rate-limit": "^8.1.0",
        "helmet": "^7.1.0",
        "ioredis": "^5.8.0",
        "morgan": "^1.10.0",
        "node-fetch": "^2.7.0",
        "pg": "^8.16.3",
        "rate-limit-redis": "^4.2.2",
        "redis": "^5.8.3",
        "stripe": "^18.5.0"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/postcss": "^4.1.13",
        "@tailwindcss/typography": "^0.5.16",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/lru-cache": "^7.10.9",
        "@types/node": "^20.19.9",
        "@types/pg": "^8.15.4",
        "@types/ws": "^8.5.13",
        "@vitejs/plugin-react": "^4.7.0",
        "autoprefixer": "^10.4.21",
        "basic-ftp": "^5.0.5",
        "better-sqlite3": "^12.4.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "concurrently": "^9.2.1",
        "dotenv-cli": "^9.0.0",
        "drizzle-kit": "^0.30.4",
        "esbuild": "^0.25.0",
        "nodemon": "^3.1.10",
        "postcss": "^8.5.6",
        "sqlite3": "^5.1.7",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "tailwindcss-animate": "^1.0.7",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.1",
        "typescript": "^5.8.3",
        "vite": "^5.4.19",
        "vitest": "^2.1.8"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "keywords": [
        "ai",
        "saas",
        "prompt-engineering",
        "openai",
        "anthropic",
        "stripe",
        "typescript",
        "react"
    ],
    "author": "SmartPromptIQ Team",
    "license": "MIT",
    "main": "create-admin.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Wthomas48/smartpromptiq-pro.git"
    },
    "bugs": {
        "url": "https://github.com/Wthomas48/smartpromptiq-pro/issues"
    },
    "homepage": "https://github.com/Wthomas48/smartpromptiq-pro#readme"
}
